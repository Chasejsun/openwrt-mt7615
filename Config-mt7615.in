	default WIFI_PAGE_ALLOC_SKB

	config  WIFI_SLAB_ALLOC_SKB
		bool "SLAB skb allocation"

	config  WIFI_PAGE_ALLOC_SKB
		bool "Page skb allocation"
endchoice

config RTDEV
	bool
	default y if WIFI_DRIVER && !SECOND_IF_NONE || RTDEV_MII
	default y if RTDEV_PLC

